    "vite": "^3.0.0",
    "vitepress": "1.0.0-alpha.4",
    "vue": "^3.2.37",
    "vue-tsc": "^0.38.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite",
        "@algolia/client-search",
