{
  "name": "arco-vue-pro-components",
  "description": "基于arco-design-vue二次封装的组件库",
  "version": "0.0.2",
  "files": ["dist"],
  "main": "./dist/arco-vue-pro-components.umd.js",
  "module": "./dist/arco-vue-pro-components.es.js",
  "typings":"./dist/components/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/arco-vue-pro-components.es.js",
      "require": "./dist/arco-vue-pro-components.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.33.1",
    "@arco-plugins/vite-vue": "^1.3.5",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.12.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.7.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vitepress": "1.0.0-alpha.4",
    "vue": "^3.2.37",
    "vue-tsc": "^0.38.4",
    "@types/lodash": "^4.14.182",
    "less": "^4.1.3",
    "lodash": "^4.17.21"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite",
        "@algolia/client-search",
        "@types/react",
        "react",
        "react-dom"
      ]
    }
  }
}